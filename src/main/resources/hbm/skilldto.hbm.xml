<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.cvut.fit.bpm.api.dto.SkillDto">
		<id column="SKILL_ID" name="id" type="java.lang.Long">
			<generator class="increment" />
		</id>
		<property name="upperSkillId" type="java.lang.Integer" />
		<property name="type" type="java.lang.String" />
		<property name="rating" type="java.lang.Integer" />
		<property name="mandatory" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>