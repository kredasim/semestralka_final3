<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cz.cvut.fit.bpm.api.dto.RoleDto">
		<id column="ROLE_ID" name="id" type="java.lang.Long">
			<generator class="increment"/>
		</id>
		<property column="ROLE_NAME" name="name" type="java.lang.String" />
		<property column="ROLE_DESCRIPTION" name="description" type="java.lang.String" />
	</class>

</hibernate-mapping>